    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/platform-express": "^10.3.8",
    "@novai/shared": "*",
    "@supabase/supabase-js": "^2.43.0",
    "@types/fluent-ffmpeg": "^2.1.28",
    "archiver": "^7.0.1",
    "bullmq": "^5.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fluent-ffmpeg": "^2.1.3",
    "ioredis": "^5.4.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
